!!! Transitional
%html
  %head
    %title Tradeskills Ensmartle Me
    =javascript_include_tag :defaults
    =stylesheet_link_tag 'reset', 'ensmartle'
  %body
    #header
      =link_to image_tag("logo2.png"), "/"
      =link_to "Add Character", add_character_path
      -form_tag search_path, :method => :get do
        =text_field_tag :q, "", :autocomplete => "off"
        %button Go
    #content
      %p#messages{:style=>"color: green"}=flash[:notice]
      =yield
    = javascript_include_tag "http://ensmartle.shouldbeuservoicedotcom/pages/general/widgets/tab.js?alignment=right&amp;color=00BCBA"